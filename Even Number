class Even
{
  public static void main(String[] args)
  {
     Scanner sc=new Scanner(System.in);
     int a=sc.nextInt();

     if(a%2==0)
     {
       System.out.println("Given number is Even number");
     }
     else
     {
       System.out.println("Given Number is odd Number:");
     }
  }
}
